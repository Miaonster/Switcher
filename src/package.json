    "min_height": 600
  },
  "webkit": {
    "plugin": true
  },
  "dependencies": {
    "seajs": "~2.1.1",
    "marked": "~0.2.9",
    "sprintf": "~0.1.3",
    "deferred": "~0.6.6",
    "jquery": "~2.1.0-beta3",
    "JQDeferred": "~1.9.1"
  }
}
