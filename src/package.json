{
  "main": "index.html",
  "name": "Switcher",
  "author": "witcher42",
  "description": "Host switcher based on node-webkit.",
  "version": "1.0.0",
  "keywords": [
    "Host Switcher",
    "node-webkit"
  ],
  "window": {
